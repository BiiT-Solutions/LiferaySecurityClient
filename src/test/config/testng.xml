<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="WebServiceSuite">
	<test name="AppfoundationTest" enabled="true" preserve-order="true">
		<groups>
			<run>
				<include name="connection" />
				<include name="companyAccess" />
				<include name="userAccess" />
				<include name="basicSecurityMethodTest" />
				<include name="roleAccess" />
				<include name="clearData" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.liferay.access.AccessTest" />
			<class name="com.biit.liferaysecurity.BasicSecurityMethodTest" />
		</classes>
	</test>
</suite>
