<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="1" name="WebServiceSuite">
	<test name="AppfoundationTest" enabled="true" preserve-order="true">
		<groups>
			<run>
				<!-- Disabled as they need a Liferay instance running -->
				<exclude name="connection" />
				<exclude name="companyAccess" />
				<exclude name="siteAccess" />
				<exclude name="userAccess" />
				<exclude name="basicSecurityMethodTest" />
				<exclude name="roleAccess" />
				<exclude name="groupAccess" />
				<exclude name="contactAccess" />
				<exclude name="organizationAccess" />
				<exclude name="pool" />
				<exclude name="clearData" />
			</run>
		</groups>
		<classes>
			<class name="com.biit.liferay.access.AccessTest" />
			<class name="com.biit.liferay.access.AccessUtilsTest" />
		</classes>
	</test>
</suite>

